{
  "workbench.colorCustomizations": {
    "statusBar.background": "#124066",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238",
    // Color of the editor selection.
    "editor.selectionBackground": "#443636",
    "editorLineNumber.foreground": "#09ff01",
    // Color for regions with the same content as the selection.
    "editor.selectionHighlightBackground": "#1090a157",
    // Color of the current search match.
    "editor.findMatchBackground": "#528815",
    // Color of the other search matches.
    "editor.findMatchHighlightBackground": "#3404c5b7",
    // Background color of a symbol during read-access,
    // for example when reading a variable.
    "editor.wordHighlightBackground": "#253a1c",
    // Background color of a symbol during write-access,
    // for example when writing to a variable.
    "editor.wordHighlightStrongBackground": "#151e7ca8",
    "editorBracketMatch.border": "#c40909",
    "terminal.foreground": "#94a39a",
    "terminal.background": "#383737",
    "list.activeSelectionBackground": "#1d6e0d",
    "panel.border": "#f80404",
    "panel.background": "#202020",
    "panelTitle.activeBorder": "#00ff",
    "tab.activeBackground": "#1f80dbaa",
    "tab.border": "#014c74",
    "panelTitle.activeForeground": "#0272f1",
    "panelTitle.inactiveForeground": "#383737"
  },
  "workbench.colorTheme": "XhiTheme Color",
  "workbench.iconTheme": "ayu",
